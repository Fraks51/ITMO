EditPost,\r\n            Index,\r\n            Enter,\r\n            Register,\r\n            Sidebar,\r\n            AddPost,\r\n            Users,\r\n            MainPage\r\n        }, beforeCreate() {\r\n            this.$root.$on(\"onChangePage\", (page) => {\r\n                this.page = page;\r\n            });\r\n            this.$root.$on(\"onRegisterSuccess\", () => {\r\n                this.page.page = \"Enter\";\r\n            });\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}